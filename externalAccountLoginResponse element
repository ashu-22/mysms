<externalAccountLoginResponse>
  <errorCode>...</errorCode>
  <msisdn>...</msisdn>
  <authToken>...</authToken>
  <info>...</info>
  <dateVerified>...</dateVerified>
  <countryCode>...</countryCode>
  <dialPrefix>...</dialPrefix>
  <currencyCode>...</currencyCode>
  <email>...</email>
</externalAccountLoginResponse>
