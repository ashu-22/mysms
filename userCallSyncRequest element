    "read" : false,
    "duration" : ...,
    "status" : ...,
    "dateCall" : "..."
  }, ... ],
  "syncAcks" : [ {
    "operation" : ...,
    "callId" : ...,
    "deviceCallId" : ...
  }, ... ],
  "syncLimit" : ...,
  "syncInitial" : false,
  "authToken" : "...",
  "apiKey" : "..."
}
